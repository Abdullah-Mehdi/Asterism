modules = ["nodejs-20"]
run = "echo '⚠️  Use the DEPLOY button instead of Run for production! ⚠️' && echo 'The Run button is disabled to prevent duplicate bot instances.' && echo 'Go to the Deployments tab and use the deployed version.'"

[nix]
channel = "stable-24_05"

[deployment]
run = ["sh", "-c", "npm run start"]
build = ["sh", "-c", "npm install"]
deploymentTarget = "gce"
ignorePorts = true

[agent]
expertMode = true
